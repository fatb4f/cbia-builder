{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CBIA References Manifest (legacy ssot-input/references.json)",
  "type": "object",
  "required": [
    "manifest_id",
    "generated_at",
    "refs"
  ],
  "properties": {
    "manifest_id": {
      "type": "string",
      "minLength": 1
    },
    "generated_at": {
      "type": "string"
    },
    "refs": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "ref_id",
          "title",
          "url"
        ],
        "properties": {
          "ref_id": {
            "type": "string",
            "minLength": 1
          },
          "title": {
            "type": "string",
            "minLength": 1
          },
          "url": {
            "type": "string",
            "minLength": 1
          },
          "retrieval_mode": {
            "type": "string"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}
